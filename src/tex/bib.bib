@article{Nakajima2015,
	title = {Melting and mixing states of the {Earth}'s mantle after the {Moon}-forming impact},
	volume = {427},
	issn = {0012821X},
	url = {http://www.sciencedirect.com/science/article/pii/S0012821X15003805},
	doi = {10.1016/j.epsl.2015.06.023},
	abstract = {The Earth's Moon is thought to have formed by an impact between the Earth and an impactor around 4.5 billion years ago. This impact could have been so energetic that it could have mixed and homogenized the Earth's mantle. However, this view appears to be inconsistent with geochemical studies that suggest that the Earth's mantle was not mixed by the impact. Another outcome of the impact is that this energetic impact melted the whole mantle, but the extent of mantle melting is not well understood even though it must have had a significant effect on the subsequent evolution of the Earth's interior and atmosphere. To understand the initial state of the Earth's mantle, we perform giant impact simulations using smoothed particle hydrodynamics (SPH) for three different models: (a) standard: a Mars-sized impactor hits the proto-Earth, (b) fast-spinning Earth: a small impactor hits a rapidly rotating proto-Earth, and (c) sub-Earths: two half Earth-sized planets collide. We use two types of equations of state (MgSiO3 liquid and forsterite) to describe the Earth's mantle. We find that the mantle remains unmixed in (a), but it may be mixed in (b) and (c). The extent of mixing is most extensive in (c). Therefore, (a) is most consistent and (c) may be least consistent with the preservation of the mantle heterogeneity, while (b) may fall between. We determine that the Earth's mantle becomes mostly molten by the impact in all of the models. The choice of the equation of state does not affect these outcomes. Additionally, our results indicate that entropy gains of the mantle materials by a giant impact cannot be predicted well by the Rankine–Hugoniot equations. Moreover, we show that the mantle can remain unmixed on a Moon-forming timescale if it does not become mixed by the impact.},
	urldate = {2015-07-30},
	journal = {Earth and Planetary Science Letters},
	author = {Nakajima, Miki and Stevenson, David J.},
	year = {2015},
	keywords = {deep Earth, mantle heterogeneity, Moon, satellite formation, thermodynamics},
	pages = {286--295},
	file = {2015-Melting_and_mixing_states_of_the_Earths_mantle_after_the_Moon-forming_impact.pdf:/Users/vq21447/Zotero/storage/VT73TSPD/2015-Melting_and_mixing_states_of_the_Earths_mantle_after_the_Moon-forming_impact.pdf:application/pdf;2015-Melting_and_mixing_states_of_the_Earths_mantle_after_the_Moon-forming_impact.pdf:/Users/vq21447/Zotero/storage/H9ZMWRAG/2015-Melting_and_mixing_states_of_the_Earths_mantle_after_the_Moon-forming_impact.pdf:application/pdf},
}

