	keywords = {Astrophysics - Earth and Planetary Astrophysics, Disk self-gravity, Jovian planets, Jupiter, Planet-disk interaction, Planetary formation},
	pages = {1--25},
	file = {arXiv Fulltext PDF:/Users/vq21447/Zotero/storage/DFYFSGR4/D'Angelo and Lissauer - 2018 - Formation of Giant Planets.pdf:application/pdf;arXiv.org Snapshot:/Users/vq21447/Zotero/storage/NQD2492N/1806.html:text/html},
}

@article{Izidoro2015,
	title = {Accretion of {Uranus} and {Neptune} from inward-migrating planetary embryos blocked by {Jupiter} and {Saturn}},
	volume = {582},
	copyright = {© ESO, 2015},
	issn = {0004-6361, 1432-0746},
	url = {https://www.aanda.org/articles/aa/abs/2015/10/aa25525-14/aa25525-14.html},
	doi = {10.1051/0004-6361/201425525},
	abstract = {Reproducing Uranus and Neptune remains a challenge for simulations of solar system formation. The ice giants’ peculiar obliquities suggest that they both suffered giant collisions during their formation. Thus, there must have been an epoch of accretion dominated by collisions among large planetary embryos in the primordial outer solar system. We test this idea using {\textless}i{\textgreater}N{\textless}i/{\textgreater}-body numerical simulations including the effects of a gaseous protoplanetary disk. One strong constraint is that the masses of the ice giants are very similar – the Neptune and Uranus mass ratio is {\textasciitilde}1.18. We show that similar-sized ice giants do indeed form by collisions between planetary embryos beyond Saturn. The fraction of successful simulations varies depending on the initial number of planetary embryos in the system, their individual and total masses. Similar-sized ice giants are consistently reproduced in simulations starting with five to ten planetary embryos with initial masses of {\textasciitilde}3–6 {\textless}i{\textgreater}M{\textless}i/{\textgreater}{\textless}sub{\textgreater}⊕{\textless}sub/{\textgreater}. We conclude that accretion from a population of planetary embryos is a plausible scenario for the origin of Uranus and Neptune.},
	language = {en},
	urldate = {2023-03-24},
	journal = {Astronomy \& Astrophysics},
	author = {Izidoro, André and Morbidelli, Alessandro and Raymond, Sean N. and Hersant, Franck and Pierens, Arnaud},
	month = oct,
	year = {2015},
